# Local environment variables. Copy this file to `.env.local` and set real values.

# SQLite database used by Prisma locally.
DATABASE_URL="file:./prisma/dev.db"

# Auth secret for JWT signing (replace with a strong random string).
AUTH_SECRET="replace-with-strong-secret"

# Optional Gemini API keys and defaults.
GEMINI_API_KEY="your-gemini-api-key-here"
GOOGLE_API_KEY=""
GEMINI_MODEL="gemini-2.0-flash-exp"
GEMINI_MAX_TOKENS=2048
GEMINI_TEMPERATURE=0.7
